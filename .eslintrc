{
    "extends": "airbnb",
    "rules": {
        "no-console":0,
        "arrow-body-style": 0,
        "indent": ["error", 4],
        "no-param-reassign": 0,
        "react/jsx-indent":[1,4],
        "react/prefer-stateless-function":0,
        "react/jsx-indent-props":0,
        "react/jsx-closing-bracket-location":0
    }
}